{
    "name": "deworkerpro/demo-auction",
    "description": "",
    "type": "project",
    "license": "BSD-3-Clause",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "doctrine/migrations": "^3.0",
        "doctrine/orm": "^2.7",
        "finesse/swiftmailer-defaults-plugin": "^2.0",
        "laminas/laminas-config-aggregator": "^1.2",
        "middlewares/negotiation": "^2.0",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.0",
        "ramsey/uuid": "^4.0",
        "sentry/sdk": "^3.1",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.3",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/translation": "^5.0",
        "symfony/validator": "^5.0",
        "twig/twig": "^3.0",
        "webmozart/assert": "^1.6"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.2.0",
        "doctrine/data-fixtures": "^1.4",
        "guzzlehttp/guzzle": "^7.2",
        "lctrs/psalm-psr-container-plugin": "^1.1",
        "overtrue/phplint": "^2.0",
        "phpunit/phpunit": "^9.2",
        "psalm/plugin-phpunit": "^0.15.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phplint",
        "phpcs": "phpcs --standard=phpcs.xml",
        "phpcbf": "phpcbf --standard=phpcs.xml",
        "psalm": "psalm --config=psalm.xml",
        "test": "phpunit --colors=always",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --colors=always --coverage-html var/test/coverage",
        "app": "php bin/app.php --ansi"
    }
}
